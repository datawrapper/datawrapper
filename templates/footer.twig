<p>{% trans "footer / datawrapper-gmbh" %}</p>
{% if config.footer_links %}
<div class="footer-links">
  {% for link in config.footer_links %}
    <a {% if link.highlight %}class="highlight"{% endif %} href="{{ link.url | replace({'%lang': language }) }}">{{ link.title[language] }}</a>
  {% endfor %}
</div>
{% endif %}
