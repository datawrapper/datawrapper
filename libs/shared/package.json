{
    "name": "@datawrapper/shared",
    "version": "1.3.0",
    "description": "shared functions used throughout datawrapper",
    "keywords": [
        "js",
        "shared",
        "utils"
    ],
    "homepage": "https://github.com/datawrapper/shared#readme",
    "bugs": {
        "url": "https://github.com/datawrapper/shared/issues"
    },
    "license": "MIT",
    "author": "Datawrapper GmbH",
    "main": "dist/mjs/index.js",
    "files": [
        "*.d.ts",
        "src/**/*.ts",
        "src/**/*.js",
        "dist/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.cjs"
    ],
    "type": "module",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=7.0.0"
    },
    "scripts": {
        "test": "ava",
        "test:watch": "ava -w",
        "build": "rm -Rf dist/* && tsc && tsc --outDir dist/cjs --module commonjs && renamer --view one-line --find js --replace cjs dist/cjs/*.js",
        "prepublishOnly": "npm run lint && npm run build && npm test && ./scripts/declare-exports.cjs && cp dist/mjs/*.d.ts .",
        "postpublish": "rm *.d.ts",
        "lint": "(cd ../.. && scripts/lint libs/shared)",
        "format": "(cd ../.. && scripts/format libs/shared)",
        "docs": "jsdoc2md --template jsdoc2md/README.hbs --files ./*.[c]js -g grouped --separators | sed '/\\*\\*Kind\\*\\*/d' | sed '/## $/d' | sed 's/## \\([a-z]\\)/### \\1/' > README.md && node jsdoc2md/fixSorting.cjs"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/datawrapper/shared.git"
    },
    "devDependencies": {
        "@types/numeral": "^2.0.2",
        "ava": "^4.3.3",
        "ava-fast-check": "^6.0.0",
        "browser-env": "^3.2.6",
        "d3-interpolate": "^1.3.2",
        "dayjs": "^1.11.3",
        "esm": "3.2.25",
        "fast-check": "^3.1.2",
        "form-data": "^4.0.0",
        "jsdoc-to-markdown": "^7.1.1",
        "nock": "^13.2.9",
        "numeral": "^2.0.6",
        "raf": "^3.4.1",
        "renamer": "^4.0.0",
        "sinon": "^14.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "chroma-js": "^2.4.2",
        "d3-array": "^1.2.4",
        "domutils": "^2.8.0",
        "fontfaceobserver": "^2.1.0",
        "htmlparser2": "^7.2.0",
        "isomorphic-dompurify": "^0.22.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.21",
        "node-fetch": "^2.6.0",
        "underscore": "^1.13.1"
    },
    "ava": {
        "files": [
            "src/**/*.test.js",
            "src/**/*.test.cjs",
            "src/**/*.test.ts"
        ],
        "require": [
            "esm",
            "./test/helpers/setup-browser-env.js"
        ],
        "nodeArguments": [
            "--experimental-loader=ts-node/esm",
            "--experimental-specifier-resolution=node",
            "--no-warnings"
        ]
    },
    "publishConfig": {
        "access": "public"
    },
