#!/bin/sh
if [ -f /usr/bin/coffee ]
then
	/usr/bin/coffee -o ./ -j coffee-compiled.js -c ./coffee
fi
cat dw.core.js dw.dataset.js dw.ui.js dw.chart.js dw.chart.editable.js dw.theme.js dw.parsers.delimited.js dw.visualization.js coffee-compiled.js > _.js
uglifyjs _.js > ../datawrapper.min.js
cat dw.core.js dw.dataset.js dw.chart.js dw.theme.js dw.parsers.delimited.js dw.visualization.js > ../dw.js
uglifyjs ../dw.js > ../dw.min.js
rm _.js
