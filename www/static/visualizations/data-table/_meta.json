{
    "title": {
        "en": "Data Table",
        "de": "Tabelle",
        "fr": "Table de données"
    },
    "version": "0.8",
    "author": {
        "name": "gka",
        "email": "gka@vis4.net"
    },
    "license": {
        "name": "MIT",
        "url": "http://www.opensource.org/licenses/mit-license.php"
    },
    "about": "http://datatables.net",
    "libraries": [
        "datatables/jquery.dataTables.min.js"
    ],
    "options": {
        "table-sortable": {
            "type": "checkbox",
            "label": {
                "en": "Make columns sortable",
                "de": "Spalten sortierbar",
                "fr": "Colonnes triables"
            }
        },
        "table-responsive": {
            "type": "checkbox",
            "label": {
                "en": "Use responsive layout",
                "de": "Responsives Layout",
                "fr": "Disposition réactive"
            }
        },
        "table-paginate": {
            "type": "checkbox",
            "label": {
                "en": "Display content over multiple pages",
                "de": "Daten auf mehrere Seiten verteilen",
                "fr": "Étendre sur les côtés"
            }
        },
        "table-filter": {
            "type": "checkbox",
            "label": {
                "en": "Show filter",
                "de": "Suchfilter anzeigen",
                "fr": "Filtres de recherche"
            }
        }
    },
    "compatibility": {
        "msie": "9+",
        "moz": "5+",
        "webkit": "3+",
        "ios": "no"
    }
}