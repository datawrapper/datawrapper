{
    "title": {
        "en": "Donut Chart",
        "de": "Donut-Diagramm",
        "fr": "Graphique en anneau"
    },
    "version": "1.2.9",
    "extends": "pie-chart",
    "order": 60,
    "dimensions": 1,
    "options": {
        "show-total": {
            "type": "checkbox",
            "label": {
                "en": "Show total value in center",
                "de": "Zeige Summe in der Mitte",
                "fr": "Afficher la somme au centre"
            },
            "default": true
        },
        "custom-total": {
            "type": "checkbox",
            "label": {
                "en": "Use custom total value instead of sum",
                "de": "Eigenen Wert anstelle der Summe verwenden",
                "fr": "Afficher une autre valeur au centre"
            },
            "default": false,
            "depends-on": {
                "show-total": true,
                "chart.max_row_num": 1
            }
        },
        "custom-total-value": {
            "type": "text",
            "label": {
                "en": "Custom total value",
                "de": "Eigener Wert",
                "fr": "Valeur personnalis√©e"
            },
            "depends-on": {
                "show-total": true,
                "custom-total": true
            }
        }
    },
    "libraries": []
}