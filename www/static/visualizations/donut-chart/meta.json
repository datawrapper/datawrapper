{
    "title": {
        "en": "Donut Chart",
        "de": "Donut-Diagramm",
        "fr": "Graphique en anneau"
    },
    "version": "0.9.1",
    "extends": "pie-chart",
    "author": {
        "name": "gka",
        "email": "gka@vis4.net"
    },
    "license": {
        "name": "MIT",
        "url": "http://www.opensource.org/licenses/mit-license.php"
    },
    "dimensions": 1,
    "options": {
        "show-total": {
            "type": "checkbox",
            "label": {
                "en": "Show total value in center",
                "de": "Zeige Summe in der Mitte",
                "fr": "Afficher la somme au centre"
            },
            "default": true
        },
        "custom-total": {
            "type": "checkbox",
            "label": {
                "en": "Use custom total value instead of sum",
                "de": "Eigenen Wert anstelle der Summe verwenden",
                "fr": "Afficher une autre valeur au centre"
            },
            "default": false,
            "depends-on": {
                "show-total": true
            }
        },
        "custom-total-value": {
            "type": "text",
            "label": {
                "en": "Custom total value",
                "de": "Eigener Wert",
                "fr": "Valeur personnalis√©e"
            },
            "depends-on": {
                "show-total": true,
                "custom-total": true
            }
        }
    },
    "libraries": [],
    "about": "http://site.com/understanding-bar-charts/"
}